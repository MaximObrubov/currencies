<!DOCTYPE html>
<html>
  <head>
    <title>Currencies</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body class="controller-<%= controller_path.gsub("/", "_") %> action-<%= action_name %>">
    <%= render('layouts/application/menu') %>
    <div class="container content">
      <%= render('layouts/application/flash') %>
      <%= yield %>
    </div>
  </body>
</html>
